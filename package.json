{
  "name": "musicserver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node --max-old-space-size=256 ./server/bin/www",
    "create": "touch ./server/public/app.js",
    "build": "browserify -t babelify -t uglifyify ./server/public/src/app.js -o ./server/public/app.js",
    "watch": "watchify -t babelify ./server/public/src/app.js -o ./server/public/app.js --debug",
    "postinstall": "npm run create && npm run build",
    "dev": "nodemon ./server/bin/www",
    "serve": "parallelshell 'npm run dev' 'npm run watch'",
    "build2": "webpack --progress --watch",
    "build:dev": "webpack --progress --watch --config webpack.config.dev.js",
    "build:pro": "webpack --progress --config webpack.config.pro.js"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.10",
    "@uirouter/core": "^5.0.11",
    "album-art": "^1.0.2",
    "angular": "^1.6.7",
    "angular-animate": "^1.6.7",
    "angular-aria": "^1.6.7",
    "angular-material": "^1.1.5",
    "angular-media-player": "^0.5.8",
    "angular-messages": "^1.6.7",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "body-parser": "~1.15.2",
    "checklist-model": "^0.11.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "i": "^0.3.5",
    "jade": "~1.11.0",
    "jwt-simple": "^0.5.1",
    "mediaserver": "^0.1.0",
    "moment": "^2.18.1",
    "morgan": "~1.7.0",
    "ngstorage": "^0.3.11",
    "please-wait": "0.0.5",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "restangular": "^1.6.1",
    "satellizer": "^0.15.5",
    "sequelize": "^3.27.0",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-angularjs-annotate": "^0.8.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1",
    "webpack-dev-middleware": "^1.11.0"
  }
}
