{
  "name": "musicserver",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node --max-old-space-size=256 ./server/bin/www",
    "create": "touch ./server/public/app.js",
    "build": "browserify -t babelify -t uglifyify ./server/public/src/app.js -o ./server/public/app.js",
    "watch": "watchify -t babelify ./server/public/src/app.js -o ./server/public/app.js --debug",
    "postinstall": "npm run create && npm run build",
    "dev": "nodemon ./server/bin/www",
    "serve": "parallelshell 'npm run dev' 'npm run watch'"
  },
  "dependencies": {
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-aria": "^1.5.5",
    "angular-material": "^1.1.3",
    "angular-messages": "^1.5.5",
    "angular-ui-router": "^1.0.0-rc.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.4.7",
    "body-parser": "~1.15.2",
    "browserify": "^14.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "i": "^0.3.5",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "nodemon": "^1.11.0",
    "parallelshell": "^2.0.0",
    "please-wait": "0.0.5",
    "restangular": "^1.6.1",
    "sequelize": "^3.27.0",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.1.8",
    "uglifyify": "^3.0.4",
    "watchify": "^3.9.0"
  }
}
